# Minimal environment configuration for Nota-AI bot
# Copy this file to .env and fill in your values

# Required variables
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
OPENAI_API_KEY=your_openai_api_key_here
ADMIN_CHAT_ID=your_telegram_chat_id_for_error_notifications

# Optional Redis configuration (default values shown)
# REDIS_URL=redis://localhost:6379/0

# Environment settings (production recommended for deployment)
ENV=production
LOG_LEVEL=INFO